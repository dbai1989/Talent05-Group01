  volume =        {947},
  year =          {2016},
  abstract =      {Excited low-spin states in 96Mo and 98Mo have been
                   studied in ???? angular correlation experiments in
                   order to determine spins and multipole mixing ratios.
                   Furthermore, from a Doppler lineshape analysis
                   effective lifetimes ?? in the femtosecond range were
                   obtained. The experimental data show a complex
                   spectrum due to configuration mixing, which is
                   confirmed by Interacting Boson Model calculations
                   based on a Skyrme energy density functional. The
                   M1-transition strengths of transitions depopulating
                   excited 2+ states to the first 2+ state are discussed
                   in terms of the proton-neutron mixed symmetry.},
  doi =           {10.1016/j.nuclphysa.2015.12.010},
  issn =          {03759474},
